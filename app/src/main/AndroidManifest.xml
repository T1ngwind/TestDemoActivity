<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testdemoactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListViewActivity"
            android:label="ListView"
            android:exported="true">
        </activity>
        <activity android:name=".RecyclerViewActivity"
            android:label="RecyclerView"
            android:exported="true">
        </activity>
        <activity android:name=".SharedPreferencesActivity"
            android:exported="true"
            android:label="SharedPreferences/Load">
        </activity>
        <activity android:name=".FileIOActivity"
            android:exported="true"
            android:label="FileIO">
        </activity>
        <activity android:name=".RegisterActivity"
            android:exported="true"
            android:label="Register">
        </activity>
        <activity android:name=".SystemActivity"
            android:exported="true"
            android:label="LoadSuccess">
        </activity>
        <activity android:name=".SQLiteActivity"
            android:label="SQLite"
            android:exported="true">
        </activity>
    </application>

</manifest>