<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testdemoactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".MusicPlayService"
            android:enabled="true"
            android:exported="true"></service>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListViewActivity"
            android:exported="true"
            android:label="ListView"></activity>
        <activity
            android:name=".RecyclerViewActivity"
            android:exported="true"
            android:label="RecyclerView"></activity>
        <activity
            android:name=".SharedPreferencesActivity"
            android:exported="true"
            android:label="SharedPreferences/Load"></activity>
        <activity
            android:name=".FileIOActivity"
            android:exported="true"
            android:label="FileIO"></activity>
        <activity
            android:name=".RegisterActivity"
            android:exported="true"
            android:label="Register"></activity>
        <activity
            android:name=".SystemActivity"
            android:exported="true"
            android:label="LoadSuccess"></activity>
        <activity
            android:name=".SQLiteActivity"
            android:exported="true"
            android:label="SQLite"></activity>

       <!-- <provider
            android:name=".MyContentProvider"
            android:authorities="cn.scu.myprovider"
            android:exported="true"></provider>-->

        <activity
            android:name=".ContentProviderActivity"
            android:exported="true"
            android:label="ContentProvider"></activity>
        <activity
            android:name=".ServiceActivity"
            android:exported="true"
            android:label="Service"></activity>
        <activity
            android:name=".ContentActivity"
            android:exported="true"
            android:label="MultiContentProvider"></activity>
        <provider
            android:authorities="com.example.testdemoactivity.provider"
            android:name=".CustomContentProvider"
            android:exported="true"></provider>
        <activity
            android:name=".MyOwnContentProviderActivity"
            android:label="MyOwnContentProvider"
            android:exported="true"></activity>
        <activity
            android:name=".ContactsContractContentProviderActivity"
            android:label="ContactsContractContentProvider"
            android:exported="true"></activity>

    </application>

</manifest>